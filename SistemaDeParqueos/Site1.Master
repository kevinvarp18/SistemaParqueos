                                    <%
                    End If
                                         %>

                                    <%

                                        'ElseIf String.Equals(Session("Usuario"), "v") Then
                                        Dim frm_SolicitarParqueo As Boolean = False

                                        For Each sesionfrm_SolicitarParqueo As String In Session.Keys
                                            If (String.Equals(sesionfrm_SolicitarParqueo, "frm_SolicitarParqueo")) Then
                                                frm_SolicitarParqueo = True
                                                cerrar = True
                                            End If
                                        Next

                                        If (frm_SolicitarParqueo) Then
                                    %>
                                    <li><a href="frm_SolicitarParqueo.aspx">Solicitar parqueo</a></li> 
