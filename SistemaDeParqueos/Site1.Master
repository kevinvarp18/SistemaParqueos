                                        For Each sesionfrm_BrindarParqueo As String In Session.Keys
                                            If (String.Equals(sesionfrm_BrindarParqueo, "frm_BrindarParqueo")) Then
                                                frm_BrindarParqueo = True
                                                cerrar = True
                                            End If
                                        Next

                                        If (frm_BrindarParqueo) Then
                                        %>
                                    <li><a href="frm_BrindarParqueo.aspx">Brindar parqueo</a></li>
<%
                End If
    %>
                                    <%
                If cerrar Then
