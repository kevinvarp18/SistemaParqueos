                                    <asp:HyperLink ID="HypLnkSolicitudes" Style="color: #efd12f" NavigateUrl="http://localhost:52086/view/frm_AdministrarSolicitudes.aspx?id=0" Text="Pendientes: " runat="server" />
                                    <asp:HyperLink ID="HypLinkVisitantes" Style="color: #efd12f" NavigateUrl="http://localhost:52086/view/frm_SolicitudesAtrasadas.aspx?" Text="Atrasados: " runat="server" />
                                </ul>
                                <%
    End If
                                %>
                                <br>
                                <ul class="nav navbar-nav navbar-right">
                                    <%
                If String.Equals(Session("Usuario"), "N") Then
                                    %>
                                    <li><a href="frm_IniciarSesion.aspx">Iniciar Sesión</a></li>
                                    <li><a href="frm_RegistrarVisitante.aspx">Registrarse</a></li>
                                    <%
                End If

                %>

                                    <%
                                        If String.Equals(Session("Usuario"), "a") Then
